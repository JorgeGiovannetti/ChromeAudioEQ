{
  "manifest_version": 2,
  "name": "Sound Equalizer",
  "version": "0.1",
  "description": "Equalize audio within your browser!",
  "permissions": ["storage","activeTab","tabCapture","tabs","<all_urls>"],
  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup":"popup.html",
      "default_icon": {
        "16": "logo/get_started16.png",
        "32": "logo/get_started32.png",
        "48": "logo/get_started48.png",
        "128": "logo/get_started128.png"
      }
    }
}
